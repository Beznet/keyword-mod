{
  "name": "Keyword Modifier",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Modify your PPC keywords in bulk.",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Keyword Modifier",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/scripts/popup.js"
	
      ]
    }
  ]
}